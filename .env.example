# Optional Gemini key for live synthesis
GEMINI_API_KEY=

# Phase 2 - Supabase
# Hosted example: https://<project-ref>.supabase.co
# Local example (supabase start): http://127.0.0.1:54321
SUPABASE_URL=
# Use anon key for read paths and service-role key for ingestion/admin jobs.
SUPABASE_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_DOCUMENTS_TABLE=embeddings

# Runtime toggles for real connectors
USE_REAL_SUPABASE=false
USE_REAL_NEO4J=false
ALLOW_SEEDED_FALLBACK=true

# Phase 2 - Neo4j
# Aura example: neo4j+s://<instance>.databases.neo4j.io
# Local example: neo4j://localhost:7687
NEO4J_URI=
NEO4J_USERNAME=
NEO4J_PASSWORD=
NEO4J_DATABASE=neo4j
NEO4J_SCAN_LIMIT=200
NEO4J_INGEST_BATCH_SIZE=500

# Optional Aura metadata
AURA_INSTANCEID=
AURA_INSTANCENAME=

# Prototype seed dataset paths (used as fallback until retrievers are upgraded)
PROTOTYPE_DOCS_PATH=data/prototype/private_documents.json
PROTOTYPE_GRAPH_PATH=data/prototype/graph_edges.json

# Ingestion tuning defaults for Phase 2 scripts
INGEST_CHUNK_SIZE=800
INGEST_CHUNK_OVERLAP=120
NETFLIX_CSV_PATH=data/netflix_titles.csv
